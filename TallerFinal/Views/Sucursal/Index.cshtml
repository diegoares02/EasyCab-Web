@model IEnumerable<TallerFinal.Models.Sucursal>
@if (Session["tipo"].ToString().Equals("Administrador"))
{
    {
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Administrador.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Empresa.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Direccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefono)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Empresa.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefono)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SucursalId }) |
            @Html.ActionLink("Details", "Details", new { id=item.SucursalId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SucursalId })
        </td>
    </tr>
}

</table>

}
else
{
    <h1>USUARIO NO VALIDO!!!!!!!!!</h1>
}
